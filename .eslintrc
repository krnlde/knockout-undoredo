{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "strict": [2, "global"],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "no-unused-labels": 1,
    "no-unused-vars": 1,
    "no-console": 1,
    "no-debugger": 1,
    "curly": 0,
    "key-spacing": 1,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 1,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "no-fallthrough": 0,
    "new-cap": 0,
    "quotes": [1, "single", "avoid-escape"]
  }
}
